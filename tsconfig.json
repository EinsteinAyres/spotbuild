{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@storage/*": [
        "src/storage/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
